{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "hooks/*": [
        "./src/hooks/*"
      ],
      "types/*": [
        "./src/types/*"
      ],
      "styles/*": [
        "./src/styles/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
